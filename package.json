{
  "name": "@higgsfield/client",
  "version": "0.1.0",
  "description": "Official Higgsfield SDK for Node.js and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "higgsfield",
    "ai",
    "api",
    "client",
    "sdk",
    "typescript",
    "image-generation",
    "video-generation"
  ],
  "author": "Higgsfield",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/higgsfield/nodejs-client.git"
  },
  "bugs": {
    "url": "https://github.com/higgsfield/nodejs-client/issues"
  },
  "homepage": "https://higgsfield.ai",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
